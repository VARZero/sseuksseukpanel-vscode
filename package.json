{
  "name": "sseuksseukpanel-vscode",
  "displayName": "SseukSseukPanel-VSCode",
  "description": "Let's WRITE code in VSCode as HANDWRITING!",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "panel": [
        {
          "id": "sseuksseuk-panel",
          "title": "쓱쓱 판",
          "icon": "$(notebook-edit)"
        }
      ]
    },
    "views": {
      "sseuksseuk-panel": [
        {
          "type": "webview",
          "id": "SseukSseuk.panelView",
          "name": "Pen Panel",
          "icon": "$(notebook-edit)",
          "contextualTitle": "SS P"
        }
      ]
    },
    "commands": [
      {
        "command": "SseukSseuk.undo",
        "category": "Pen Panel",
        "title": "Undo",
        "icon": "$(discard)"
      },
      {
        "command": "SseukSseuk.addTopLine",
        "category": "Pen Panel",
        "title": "Add Upper Line",
        "icon": "$(arrow-up)"
      },
      {
        "command": "SseukSseuk.addBotLine",
        "category": "Pen Panel",
        "title": "Add Lower Line",
        "icon": "$(arrow-down)"
      },
      {
        "command": "SseukSseuk.removeLine",
        "category": "Pen Panel",
        "title": "Remove Current Line",
        "icon": "$(trash)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "SseukSseuk.undo",
          "group": "navigation",
          "when": "view == SseukSseuk.panelView"
        },
        {
          "command": "SseukSseuk.addTopLine",
          "group": "navigation",
          "when": "view == SseukSseuk.panelView"
        },
        {
          "command": "SseukSseuk.addBotLine",
          "group": "navigation",
          "when": "view == SseukSseuk.panelView"
        },
        {
          "command": "SseukSseuk.removeLine",
          "group": "navigation",
          "when": "view == SseukSseuk.panelView"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.79.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
